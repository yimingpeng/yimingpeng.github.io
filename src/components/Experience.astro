<script>
	// Initialize AOS (Animate On Scroll) library
	document.addEventListener('DOMContentLoaded', () => {
		// Dynamically import AOS
		import('aos').then(AOS => {
			AOS.default.init({
				duration: 800,
				easing: 'ease-in-out',
				once: true,
				offset: 100,
				delay: 100
			});
		});
	});
</script>

